// print diagonal2 elements
import java.util.*;
public class Main {
  static void diagonals2(int[][] a){
    int n=a.length;
    int m=a[0].length;
    for(int i=0;i<n;i++){
      for(int j=0; j<m; j++){
        if(i+j==n-1){
        System.out.print(a[i][j]+" ");
        }
      } 
      System.out.println();
      }
  }
  public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      int m= scan.nextInt();
      int n= scan.nextInt();
      int[][] arr1= new int[m][n];
      for(int j=0; j<m;j++){
      for(int i=0; i<n;i++){
       arr1[j][i]=scan.nextInt();
      }
      }
      diagonals2(arr1);
    }
}

input:
3
3
1 2 3 4 5 6 7 8 9

output:
3 5 7
