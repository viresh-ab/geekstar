// count a space in string
import java.util.*;
public class Main {
    public static void main(String[] args){
      int count=0;
  Scanner sc = new Scanner(System.in);
  String u = sc.nextLine();
  for(int i=u.length()-1; i>=0;i--){
  if(u.charAt(i)==' '){
    count++;
  }
  }
      System.out.println(count);
}
}
